        <logger name="org.springframework" level="debug" />
    </loggers> -->
     <loggers>
        <root level="error">
            <appender-ref ref="Console" />
        </root>
        <logger name="CSR" level="error" />
        <logger name="org.apache" level="error" />
        <logger name="org.springframework" level="error" />
    </loggers>
</configuration>

